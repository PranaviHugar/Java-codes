//reading user inputs using scanner
import java.util.Scanner;
class Addnos
{
     public static void main(String args[])
    {   int a1,a2,sum=0;
        System.out.println("enter 2 nos:");
        Scanner s=new Scanner(System.in);
        a1=s.nextInt();
        a2=s.nextInt();
        sum=a1+a2;
         System.out.println("sum="+sum);
    }
}
